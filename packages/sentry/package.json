{
	"name": "@kaname-png/plugin-sentry",
	"version": "1.2.0",
	"description": "Plugin for @sapphire/framework to post error on sentry.",
	"author": "@kaname-png",
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"typedocMain": "src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./register": {
			"import": "./register.mjs",
			"require": "./register.js",
			"types": "./register.d.ts"
		}
	},
	"sideEffects": [
		"./dist/register.js",
		"./register.js",
		"./register.mjs"
	],
	"homepage": "https://github.com/kaname-png/neko-plugins/tree/main/packages/sentry",
	"scripts": {
		"lint": "eslint src --ext ts --fix",
		"build": "tsc -b src && yarn esm:register && yarn esm:default",
		"esm:register": "gen-esm-wrapper dist/register.js dist/register.mjs",
		"esm:default": "gen-esm-wrapper dist/index.js dist/index.mjs",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"tslib": "^2.6.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kaname-png/neko-plugins.git",
		"directory": "packages/sentry"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo",
		"register.*"
	],
	"engines": {
		"node": ">=16.6.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"sapphiredev",
		"plugin",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"sentry"
	],
	"bugs": {
		"url": "https://github.com/kaname-png/neko-plugins/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@sapphire/plugin-api": "^5.1.1",
		"@sapphire/plugin-scheduled-tasks": "^7.1.2",
		"@sapphire/plugin-subcommands": "^4.2.1",
		"@sentry/integrations": "^7.68.0",
		"@sentry/node": "^7.68.0"
	}
}
